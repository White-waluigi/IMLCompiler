program fibbonacci(inout var x: int32)
    global
    fun recurse(in var y: int32) returns z: int32
        do
            debugout y;
            if y > 1000
                then x := 0
            else
                z init := x + y;
                x := y;
                call recurse(z)
            endif
    endfun
    do
        x init := 1;
        call recurse(1)
endprogram