program fibbonacci()
    global
        x: int32;
        y: int32;
        tmp: int32
    do
        x init := 1;
        y init := 1;
        tmp init := 0;
        while x < 1000
            do
            debugout x;
            tmp := x;
            x := x + y;
            y := tmp
        endwhile
endprogram