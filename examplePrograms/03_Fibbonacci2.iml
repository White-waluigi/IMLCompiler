program fibbonacci()
    global
    ctr:int32;
    proc recurse(in var x: int32, in var y: int32)
        do
            debugout y;
            y := y + x;
            ctr:=ctr+1;
            if ctr < 101 then
                call recurse(y , x)
            else
                y:=9
            endif
    endproc
    do
        call recurse(1,1)
endprogram